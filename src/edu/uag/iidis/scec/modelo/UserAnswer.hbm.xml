<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">  

<hibernate-mapping>
    <class name="edu.uag.iidis.scec.modelo.UserAnswer"
           table="useranswer"
           dynamic-update="false"
           dynamic-insert="false">

         <id name="id"
            column="id"
            type="java.lang.Long"
            unsaved-value="null" >
            <generator class="increment">
            </generator>
        </id>
        
        <property name="idTry"
            type="java.lang.Long"
            not-null="false" />
            
        <property name="idAnswer"
            type="java.lang.Long"
            not-null="false" />
           
    </class>

</hibernate-mapping>
